<%= form_for @message , url: messages_path , html: {method: :post} do |f| %>

  <h1>メッセージ作成</h1>
  <p>タイトル:<%= f.text_field :title %></p>
  <p>内容:<%= f.text_field :content %></p>
  <input type="submit" name="commit" value="送信">
  <%= link_to "一覧画面へ" , messages_path %>
    
